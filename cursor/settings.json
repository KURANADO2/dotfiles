{
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "window.commandCenter": true,
  "editor.fontFamily": "JetBrains Mono",
  "markdown.preview.lineHeight": 0.9,
  "redhat.telemetry.enabled": false,
  "workbench.colorTheme": "Default Dark Modern",
  "terminal.external.osxExec": "Kitty.app",
  "editor.lineNumbers": "relative",
  // Do not enable .vimrc file, otherwise, vscode vim will encounter some strange problems
  // "vim.vimrc.enable": true,
  // leader key 设置为空格
  "vim.leader": "<space>",
  // 使用系统剪贴板
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  // cnoremap
  "vim.commandLineModeKeyBindingsNonRecursive": [],
  // imap
  "vim.insertModeKeyBindings": [],
  // nnoremap
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "V",
      ],
      "after": [
        "<C-v>"
      ]
    },
    {
      "before": [
        "<C-v>",
      ],
      "after": [
        "V"
      ]
    },
    {
      "before": [
        "<C-j>",
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "<C-k>",
      ],
      "after": [
        "5",
        "k"
      ]
    },
    {
      "before": [
        "<C-h>",
      ],
      "after": [
        "^",
      ]
    },
    {
      "before": [
        "<C-l>",
      ],
      "after": [
        "$",
      ]
    },
    {
      "before": [
        "s",
        "l",
      ],
      "commands": [
        ":vsp",
      ]
    },
    {
      "before": [
        "s",
        "j",
      ],
      "commands": [
        ":sp",
      ]
    },
    {
      "before": [
        "<leader>",
        "h",
      ],
      "after": [
        "<C-w>",
        "h",
      ]
    },
    {
      "before": [
        "<leader>",
        "j",
      ],
      "after": [
        "<C-w>",
        "j",
      ]
    },
    {
      "before": [
        "<leader>",
        "k",
      ],
      "after": [
        "<C-w>",
        "k",
      ]
    },
    {
      "before": [
        "<leader>",
        "l",
      ],
      "after": [
        "<C-w>",
        "l",
      ]
    },
    {
      "before": [
        "J",
      ],
      "after": [
        "5",
        "<C-e>"
      ]
    },
    {
      "before": [
        "K",
      ],
      "after": [
        "5",
        "<C-y>"
      ]
    },
    {
      "before": [
        "H",
      ],
      "after": [
        "2",
        "0",
        "z",
        "h"
      ]
    },
    {
      "before": [
        "L",
      ],
      "after": [
        "20zl"
      ]
    },
    // Toggle case
    {
      "before": [
        "`",
      ],
      "after": [
        "~"
      ]
    },
    // Redo
    {
      "before": [
        "U",
      ],
      "after": [
        "<C-r>"
      ]
    },
    // Format codes
    {
      "before": [
        "<leader>",
        "c",
        "f"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    // Copy
    {
      "before": [
        "Y",
      ],
      "after": [
        "v",
        "$",
        "h",
        "y"
      ]
    },
    {
      "before": [
        "v",
        "<C-l>",
      ],
      "after": [
        "v",
        "$",
        "h",
      ]
    },
    {
      "before": [
        "v",
        "<C-h>",
      ],
      "after": [
        "v",
        "^",
      ]
    },
    {
      "before": [
        "v",
        "i",
        "e",
      ],
      "after": [
        "g",
        "g",
        "V",
        "G",
      ]
    },
    {
      "before": [
        "*"
      ],
      "after": [
        "*",
        "z",
        "z",
      ]
    },
    {
      "before": [
        "#"
      ],
      "after": [
        "#",
        "z",
        "z",
      ]
    },
    {
      "before": [
        "n"
      ],
      "after": [
        "n",
        "z",
        "z",
      ]
    },
    {
      "before": [
        "N"
      ],
      "after": [
        "N",
        "z",
        "z",
      ]
    },
    {
      "before": [
        "<leader>",
        ";"
      ],
      "after": [
        "A",
        ";",
        "<esc>"
      ]
    },
    // Set s no any action
    {
      "before": [
        "s"
      ],
      "after": [
        "<nop>"
      ]
    },
    // Set s no any action
    {
      "before": [
        "\\",
        "s"
      ],
      "after": [
        ""
      ]
    },
    // Rename
    {
      "before": [
        "<leader>",
        "r",
        "n"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    // Hover
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    // Quick open
    {
      "before": [
        "<leader>",
        "<leader>"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    // Keyword search
    {
      "before": [
        "<leader>",
        "/"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    // Previous tab
    {
      "before": [
        "E"
      ],
      "commands": [
        "workbench.action.previousEditorInGroup"
      ]
    },
    // Next tab
    {
      "before": [
        "R"
      ],
      "commands": [
        "workbench.action.nextEditorInGroup"
      ]
    },
    // First tab
    {
      "before": [
        "W"
      ],
      "commands": [
        "workbench.action.firstEditorInGroup"
      ]
    },
    // Last tab
    {
      "before": [
        "T"
      ],
      "commands": [
        "workbench.action.lastEditorInGroup"
      ]
    },
    // Recent projects
    {
      "before": [
        "t",
        "p"
      ],
      "commands": [
        "workbench.action.openRecent"
      ]
    },
    // Reveal in explorer view
    {
      "before": [
        "s",
        "s"
      ],
      "commands": [
        "workbench.files.action.showActiveFileInExplorer"
      ]
    },
    // Go back
    {
      "before": [
        "<ctrl-o>",
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    // Go forward
    {
      "before": [
        "<ctrl-i>",
      ],
      "commands": [
        "workbench.action.navigateForward"
      ]
    },
    // Git annotations
    {
      "before": [
        "<leader>",
        "a",
      ],
      "commands": [
        "gitlens.toggleFileBlame"
      ]
    },
    // Git next chagne
    {
      "before": [
        "g",
        "j",
      ],
      "commands": [
        "gitlens.annotations.nextChange"
      ]
    },
    // Git previous chagne
    {
      "before": [
        "g",
        "k",
      ],
      "commands": [
        "gitlens.annotations.previousChange"
      ]
    },
    // File history
    {
      "before": [
        "g",
        "h",
      ],
      "commands": [
        "gitlens.showQuickFileHistory"
      ]
    },
    // Git discard changes
    {
      "before": [
        "<leader>",
        "r",
        "b",
      ],
      "commands": [
        "git.clean"
      ]
    },
    // 保存文件
    {
      "before": [
        "S"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    // 关闭当前标签页
    {
      "before": [
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    // Close other tabs
    {
      "before": [
        "c",
        "o"
      ],
      "commands": [
        "workbench.action.closeOtherEditors"
      ]
    },
    // Close all editors in group
    {
      "before": [
        "c",
        "g"
      ],
      "commands": [
        "workbench.action.closeEditorsInGroup"
      ]
    },
    // Close left editors
    {
      "before": [
        "c",
        "h"
      ],
      "commands": [
        "workbench.action.closeEditorsToTheLeft"
      ]
    },
    // Close right editors
    {
      "before": [
        "c",
        "l"
      ],
      "commands": [
        "workbench.action.closeEditorsToTheRight"
      ]
    },
    // Code action
    {
      "before": [
        "<leader>",
        "c",
        "a"
      ],
      "commands": [
        "editor.action.quickFix"
      ]
    },
    // Close window
    {
      "before": [
        "Q"
      ],
      "commands": [
        "workbench.action.closeWindow"
      ]
    },
  ],
  "vim.commandLineModeKeyBindings": [],
  "dotnetAcquisitionExtension.proxyUrl": "http://127.0.0.1:7890",
  "git.autofetch": true
}